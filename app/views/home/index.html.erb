<div class="panel panel-default">
  <div class="panel-heading">
    <h3 class="panel-title">
    	<%= link_to 'Click Here To Generate A New Superhero', root_path %>
	</h3>
  </div>
  <div class="panel-body">
    <%= @avatar %>
    <br/>
    <br/>
    <br/>
    Name: <%= @hero_name %>
    <br/>
    Power: <%= @hero_power %>
    <br/>
    Day Job: <%= @hero_corp_title %>
    <br/>
    Corp Power: <%= @hero_corp_power %>
  </div>
  <div class="panel-footer">
  	<%= link_to 'Create A Whole Superhero Team', home_team_path %>
  </div>
</div>
